    "prettier": "^2.0.4",
    "react-custom-checkbox": "^3.1.5",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-insta-stories": "^2.6.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.71.1"
  },
